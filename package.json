{
  "name": "@mihanizm56/fetch-api",
  "version": "1.0.1",
  "description": "api servise",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:watch": "jest --watch",
    "test": "set CI=true && jest",
    "test:coverage": "jest --coverage",
    "compile": "tsc",
    "lint": "npx eslint . --ext .ts --fix",
    "commit": "git add . && git commit --no-edit",
    "packages-vulnerability-check": "check-audit",
    "publish":"npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mihanizm56/api-service.git"
  },
  "author": "mihanizm56",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mihanizm56/api-service/issues"
  },
  "homepage": "https://github.com/mihanizm56/api-service#readme",
  "dependencies": {
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@types/jest": "24.9.1",
    "@types/node": "12.11.7",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "commitizen": "4.0.3",
    "cz-customizable": "6.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "3.0.9",
    "jest": "25.1.0",
    "npm-audit-resolver": "2.1.0",
    "prettier": "1.18.2",
    "ts-jest": "25.2.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  }
}
